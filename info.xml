<?xml version="1.0"?>
<extension key="nihrbackbone" type="module">
  <file>nihrbackbone</file>
  <name>NIHR BioResource CiviCRM Backbone</name>
  <description>CiviCRM extension for NIHR BioResource Backbone eh</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Erik Hommel</author>
    <email>erik.hommel@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/partners/civicoop/nihr-bioresearch/nihrbackbone</url>
    <url desc="Documentation">https://lab.civicrm.org/partners/civicoop/nihr-bioresearch/nihrbackbone</url>
    <url desc="Support">https://civicoop.org</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-06-08</releaseDate>
  <version>2.52</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <requires>
    <ext>de.systopia.identitytracker</ext>
  </requires>
  <comments>Extension is being tested</comments>
  <civix>
    <namespace>CRM/Nihrbackbone</namespace>
    <format>22.12.1</format>
  </civix>
  <upgrader>CRM_Nihrbackbone_Upgrader</upgrader>
</extension>
